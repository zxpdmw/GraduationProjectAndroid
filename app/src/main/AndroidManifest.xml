<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="top.zxpdmw.graduationproject">

    <uses-sdk tools:overrideLibrary="com.xuexiang.xui." />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity
            android:name=".activity.HouseRentSaleActivity"
            android:exported="true" />
        <activity
            android:name=".activity.HouseKeepingActivity"
            android:exported="true" />
        <activity android:name=".activity.RepairActivity"
            android:exported="true"/>
        <activity
            android:name=".activity.ComplainActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MyInfoActivity"
            android:exported="true" />
        <activity
            android:name=".activity.CommunityPageActivity"
            android:exported="true" />
        <activity
            android:name=".activity.DetailNoticeActivity"
            android:exported="true" />
        <activity
            android:name=".activity.PropertyActivity"
            android:exported="true" />
        <activity
            android:name=".activity.NoticeActivity"
            android:exported="true"
            android:parentActivityName=".activity.SystemMainActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SystemMainActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>