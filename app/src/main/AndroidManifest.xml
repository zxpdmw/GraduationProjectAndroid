<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="top.zxpdmw.graduationproject">

    <uses-sdk tools:overrideLibrary="com.xuexiang.xui." />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity
            android:name=".ui.activity.system.WeatherActivity"
            android:exported="true"></activity>
        <activity
            android:name=".ui.activity.system.PropertyActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.CommunityPageActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.DetailNoticeActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.NoticeActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.HouseRentSaleActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.HouseKeepingActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.ComplainRepairActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.system.MyInfoActivity"
            android:exported="true" />
        <activity android:name=".ui.activity.system.SystemMainActivity" />
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>